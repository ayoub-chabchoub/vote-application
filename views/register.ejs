<% include header.ejs %>
    <div class="container">
        <div class="row" id="register_row">
            <div class="col-md-6" id="cam_col">
                <video autoplay id="camera"></video>
                <button id="take_snapshots" class="btn btn-secondary btn-lg btn-block">Capture</button>
                <canvas id='snapshots'></canvas>
            </div>
            <div class="col-md-6" id="reg_col">
                <div class="jumbotron jumbotron-fluid">
                    <div class="container">
                        <h1 class="display-4">Register</h1>
                        <p class="lead">

                            <form method="post" action="/register" enctype="multipart/form-data">
                                <div class="form-group">
                                    <label for="name">NAME</label>
                                    <input type="text" class="form-control" id="name" name="name" placeholder="Enter Name">
                                    <small class="form-text text-muted">Name as on your  Card</small>
                                </div>
                                <div class="form-group" hidden>
                                    <input type="hidden" class="form-control" id="avatar" name="avatar" filename="name">
                                </div>
                                <div class="form-group">
                                    <label for="aadhaar">Cin</label>
                                    <input type="text" class="form-control" id="aadhaar" name="aadhaar" placeholder="Enter Aadhaar Number">
                                </div>
                                <div class="input-group mb-3">
                                    <div class="input-group-prepend">
                                        <label  for="constituency">
                                                constituency</label>
                                    </div>
                                    <select  id="constituency" name="constituency">
                                        <option selected>Choose...</option>
                                        <option value="Jabalpur">tunis</option>
                                        <option value="Delhi">mahdia</option>
                                    </select>
                                </div>
                                <div class="form-group">
                                    <button type="submit" id="register_btn" class="btn btn-success btn-lg btn-block" disabled>Submit</button>
                                </div>
                            </form>
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <footer class="container-fluid sp-md tc-bg-4 clearfix" data-toggle="collapse">
            <center><div class="footer-top"><a class="footer-logo" href="#"><img src="assets/img/logo.png" alt="Logo"  width="200" height="40" /></a></div></center>
        <div class="container tc-bg-5 epd-x  pd-y-20 footer-top">
            <div class="col-md-4 pd-x-l-110 epd-x-r">
                <i class="fa fa-phone"></i>
                <div class="dib">
                    <p class="ff-ssp pd-x-l-15 lh-13 tc-7 text-uppercase fz-18 fw-b">phone</p>
                    <p class="tc-7 fz-16 fw-n pd-x-l-15 pd-y-t-10 lh-11 ff-abee">(+1-888-77-8989 00)</p>
                </div>
            </div>
            <div class="col-md-4 pd-x-l-75 epd-x-r">
                <i class="flaticon-location-marker"></i>
                <div class="dib">
                    <p class="ff-ssp pd-x-l-15 lh-13 tc-7 text-uppercase fz-18 fw-b">address</p>
                    <p class="tc-7 fz-16 fw-n pd-x-l-15 pd-y-t-10 lh-11 ff-abee">120D, Strikerate st, US1010A</p>
                </div>
            </div>
            <div class="col-md-4 pd-x-l-35 epd-x-r">
                <i class="fa fa-paper-plane"></i>
                <div class="dib">
                    <p class="ff-ssp pd-x-l-15 lh-13 tc-7 text-uppercase fz-18 fw-b">email</p>
                    <p class="tc-7 fz-16 fw-n pd-x-l-15 pd-y-t-10 lh-11 ff-abee">info@domianname.com</p>
                </div>
            </div>
            <div class="clr"></div>
            </div>
        </footer>
    </body>

    </html>